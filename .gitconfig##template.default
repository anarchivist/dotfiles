# vim: set ft=gitconfig ts=2 sw=2 et:
# WARNING: Do not edit this file.
# It was generated by processing {{ yadm.source }}

[user]
{% if yadm.class == "Work" %}
  name = María A. Matienzo
  email = matienzo@berkeley.edu 
{% else %}
  name = María A. Matienzo
  email = maria@matienzo.org
{% endif %}
[alias]
  co = checkout
  br = branch
  ci = commit
  st = status
  pfwl = push --force-with-lease
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file q -t
  dump = cat-file -p
[filter "lfs"]
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
  required = true
[core]
{% if yadm.os == "Darwin"%}
  excludesfile = /Users/{{ yadm.user }}/.gitignore_global
{% else %}
  excludesfile = /home/{{ yadm.user }}/.gitignore_global
{% endif %}
[pull]
  rebase = false
[init]
  defaultBranch = main
